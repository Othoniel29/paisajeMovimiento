    public void run() {
        try {
            while(true){
                while(x<getWidth()-30){
                    Thread.sleep(50);
                    x+=10;
                    repaint();
                }
                  while(x>1){
                    Thread.sleep(50);
                    x-=10;
                    repaint();
                }
                int r = 105;
                 
                  while(r< getWidth()-4){
                     Thread.sleep(50);
                     r+=10;
                     repaint();
                     
                     }
                   while(r>2) {
                     Thread.sleep(50);
                     r-=10;
                     repaint();
                     
                     int f = 35;
                 
                  while(f< getWidth()-4){
                     Thread.sleep(50);
                     f+=10;
                     repaint();
                     
                     }
                   while(f>3) {
                     Thread.sleep(50);
                     f-=10;
                     repaint();
                   }
                   
                   int z = 220;
                 
                  while(z< getWidth()-4){
                     Thread.sleep(50);
                     z+=10;
                     repaint();
                     
                     }
                   while(z>4) {
                     Thread.sleep(50);
                     z-=10;
                     repaint();
                   }
                   
                   int h = 287;
                 
                  while(h< getWidth()-4){
                     Thread.sleep(50);
                     h+=10;
                     repaint();
                     
                     }
                   while(h>5) {
                     Thread.sleep(50);
                     h-=10;
                     repaint();
                   }
